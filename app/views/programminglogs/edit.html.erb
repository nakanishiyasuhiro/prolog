<div class="content__title">
  投稿の編集
</div>

<%= form_with model: @programminglog, class: :form, local: true do |f|%>
  <%= f.text_field :title, placeholder: :タイトル, class: :form__title %>
  <%= f.text_field :image, placeholder: :画像, class: :form__image %>
  <%= f.text_area :text, placeholder: :本文, class: :form__text %>
  <%= f.submit '投稿する', class: :submit__btn %>
<% end %>
